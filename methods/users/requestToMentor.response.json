{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "description": "Ответ на метод users.readNotify",
    "properties": {
        "error": {
            "description": "Код ошибки",
            "type": "number"
        },
        "error_text": {
            "description": "Текст ошибки",
            "type": "string"
        },
        "error_url": {
            "description": "Ссылка на страницу ошибки",
            "type": "string"
        },
        "response": {
            "additionalProperties": false,
            "properties": {
                "count": {
                    "description": "Количество ответов",
                    "type": "number"
                },
                "items": {
                    "description": "Список ответов",
                    "type": "array"
                },
                "last_id": {
                    "description": "\u041f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0439 \u0438\u0434\u0435\u043d\u0442\u0438\u0444\u0438\u043a\u0430\u0442\u043e\u0440",
                    "type": [
                        "string",
                        "integer",
                        "null"
                    ]
                }
            },
            "required": [
                "count",
                "items"
            ],
            "type": "object"
        },
        "run_time": {
            "description": "Время выполнения",
            "type": "number"
        },
        "server_time": {
            "description": "Время на сервере (UNIXTIME)",
            "type": "number"
        }
    },
    "required": [
        "response",
        "run_time",
        "server_time",
        "error",
        "error_text",
        "error_url"
    ],
    "type": "object"
}