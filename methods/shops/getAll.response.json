{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Ответ на метод shops.getAll",
    "properties": {
        "error": {
            "description": "Код ошибки",
            "type": "number"
        },
        "error_text": {
            "description": "Текст ошибки",
            "type": "string"
        },
        "error_url": {
            "description": "Ссылка на страницу ошибки",
            "type": "string"
        },
        "response": {
            "properties": {
                "count": {
                    "description": "Количество ответов",
                    "type": "number"
                },
                "items": {
                    "description": "Список ответов",
                    "items": {
                        "$ref": "objects.json#/definitions/shops/shop_light",
                        "type": "object"
                    },
                    "minItems": 0,
                    "type": "array"
                },
                "last_id": {
                    "description": "Последний идентификатор",
                    "type": [
                        "string",
                        "integer",
                        "null"
                    ]
                }
            },
            "required": [
                "last_id",
                "count",
                "items"
            ],
            "type": "object"
        },
        "run_time": {
            "description": "Время выполнения",
            "type": "number"
        },
        "server_time": {
            "description": "Время на сервере (UNIXTIME)",
            "type": "number"
        }
    },
    "required": [
        "response",
        "run_time",
        "server_time",
        "error",
        "error_text",
        "error_url"
    ],
    "type": "object"
}