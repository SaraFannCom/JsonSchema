{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Ответ на метод images.upload",
    "properties": {
        "error": {
            "description": "Код ошибки",
            "type": "number"
        },
        "error_text": {
            "description": "Текст ошибки",
            "type": "string"
        },
        "error_url": {
            "description": "Ссылка на страницу ошибки",
            "type": "string"
        },
        "response": {
            "properties": {
                "count": {
                    "description": "Количество ответов",
                    "type": "number"
                },
                "items": {
                    "description": "Список ответов",
                    "items": {
                        "properties": {
                            "id": {
                                "description": "Идентификатор новой картинки",
                                "type": "integer"
                            },
                            "image": {
                                "$ref": "objects.json#/definitions/images/image",
                                "type": [
                                    "null",
                                    "object"
                                ]
                            },
                            "web_url": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "id",
                            "image",
                            "web_url"
                        ],
                        "type": "object"
                    },
                    "type": "array"
                }
            },
            "required": [
                "count",
                "items"
            ],
            "type": "object"
        },
        "run_time": {
            "description": "Время выполнения",
            "type": "number"
        },
        "server_time": {
            "description": "Время на сервере (UNIXTIME)",
            "type": "number"
        }
    },
    "required": [
        "response",
        "run_time",
        "server_time",
        "error",
        "error_text",
        "error_url"
    ],
    "type": "object"
}