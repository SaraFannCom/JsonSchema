{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "method": "post",
    "properties": {
        "id": {
            "description": "Идентификатор сущности для отписки",
            "type": "integer"
        },
        "type": {
            "description": "Тип сущности для отписки",
            "type": "string",
            "enum": [
                "product",
                "wall_post",
                "blog_post",
                "blog",
                "image",
                "shop"
            ]
        }
    }, 
    "required": [
        "id",
        "type"
    ],
    "type": "object"
}